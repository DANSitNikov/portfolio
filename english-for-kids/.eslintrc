{
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": "warn",
    "linebreak-style": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-else-return": 0,
    "no-debugger": 0,
    "no-trailing-spaces": 0
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "airbnb-base/legacy"
}